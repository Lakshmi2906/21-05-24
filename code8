hours = int(input("Enter time in hours: ")
if hours < 0 or hours > 11:
    print("Invalid input")
else:
    hour_angle = (360 / 12) * hours
    minute_angle = (360 / 60) * hours
    angle = abs(hour_angle - minute_angle)
    min = int((angle / 11) * 60)

    print(f"({angle} / 11) or {time_in_minutes} minutes")
